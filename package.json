{
	"name": "starter-ng2-dll",
	"description": "This project aim to be a basic starting point to create Angular2 application using a vendor dll (vendor.js) containing the base Angular2 modules.",
	"version": "2.0.0-rc.5",
	"scripts": {
		"clean": "rimraf node_modules dist && npm cache clean",
		"clean-install": "npm run clean && npm install",
		"build": "rimraf dist && webpack --progress --colors --profile --bail --display-error-details --optimize-minimize --optimize-occurrence-order --optimize-dedupe --config webpack.config.app.js",
		"vendor": "webpack --progress --colors --profile --bail --display-error-details --optimize-minimize --optimize-occurrence-order --optimize-dedupe --config webpack.config.vendor.js",
		"start": "webpack-dev-server --inline --progress --colors --port 8080 --config webpack.config.app.js",
		"typings": "typings install"
	},
	"author": {
		"name": "John, Sing Dao, Siu",
		"email": "js@johnsiu.com",
		"url": "https://johnsiu.com/"
	},
	"keywords": [
		"angular",
		"ng",
		"2",
		"webpack",
		"vendor",
		"dll"
	],
	"bugs": {
		"url": "https://github.com/J-Siu/Angular-2-Vendor-Dll/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/J-Siu/Angular-2-Vendor-Dll.git"
	},
	"license": "MIT",
	"dependencies": {
		"@angular/common": "2.0.0-rc.5",
		"@angular/compiler": "2.0.0-rc.5",
		"@angular/core": "2.0.0-rc.5",
		"@angular/forms": "0.3.0",
		"@angular/http": "2.0.0-rc.5",
		"@angular/platform-browser": "2.0.0-rc.5",
		"@angular/platform-browser-dynamic": "2.0.0-rc.5",
		"@angular/router": "3.0.0-rc.1",
		"es6-shim": "^0.35.0",
		"reflect-metadata": "^0.1.3",
		"rxjs": "5.0.0-beta.6",
		"zone.js": "^0.6.12",
		"bootstrap": "^3.3.6"
	},
	"devDependencies": {
		"angular2-template-loader": "^0.4.0",
		"codelyzer": "*",
		"copy-webpack-plugin": "*",
		"css-loader": "*",
		"extract-text-webpack-plugin": "*",
		"file-loader": "*",
		"html-loader": "*",
		"html-webpack-plugin": "*",
		"json-loader": "*",
		"null-loader": "*",
		"raw-loader": "*",
		"rimraf": "*",
		"style-loader": "*",
		"ts-helpers": "^1.1.1",
		"ts-loader": "^0.8.1",
		"tslint": "^3.4.0",
		"tslint-loader": "^2.1.0",
		"typescript": "^1.8.0",
		"webpack": "*"
	}
}